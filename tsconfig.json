{
  "extends": "@cryptkeeper/config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "@cryptkeeper/providers": ["./packages/providers/src"],
      "@cryptkeeper/services": ["./packages/services/src"],
      "@src/*": ["packages/extension/src/*"]
    }
  },
  "include": ["packages/extension/src", "e2e", "packages"],
  "exclude": ["node_modules", "coverage", "dist"]
}